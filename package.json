{
  "name": "@apito/js-internal-sdk",
  "version": "1.2.0",
  "description": "A comprehensive JavaScript SDK for communicating with Apito GraphQL API endpoints",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "test": "jest --passWithNoTests",
    "lint": "eslint src --ext .ts",
    "prepare": "tsup",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "keywords": [
    "apito",
    "sdk",
    "graphql",
    "api",
    "javascript",
    "typescript",
    "headless-cms",
    "backend"
  ],
  "author": "Apito Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/apito.io/js-internal-sdk.git"
  },
  "bugs": {
    "url": "https://gitlab.com/apito.io/js-internal-sdk/issues"
  },
  "homepage": "https://docs.apito.io",
  "dependencies": {
    "axios": "^1.7.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.14.10",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "size-limit": "^11.0.2",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.2",
    "typescript": "^5.5.3"
  },
  "size-limit": [
    {
      "path": "dist/index.js",
      "limit": "50 KB"
    },
    {
      "path": "dist/index.mjs",
      "limit": "50 KB"
    }
  ]
}
